set nocompatible

syntax enable

let base16colorspace=256
colorscheme base16-tomorrow-night

set number

set wrap

set backspace=indent,eol,start

set smartindent

set tabstop=4
set softtabstop=4
set expandtab
set shiftwidth=4

filetype indent plugin on

set ruler
set incsearch
set ignorecase
set smartcase
set hlsearch
